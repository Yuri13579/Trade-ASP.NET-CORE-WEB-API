{
  "DeploymentEnvironment": "Development",
  //"DeploymentEnvironment": "Production",
  // "DeploymentEnvironment": "QA",
  "Logging": {
    // Правила, которые применяются если нет правил для конкретного провайдера
    "LogLevel": {
      "Default": "Debug", // записывать все сообщения начиная с уровня Debug и выше
      "System": "Warning", // все сообщения с категорией, которая начинается текстом System
      "Microsoft": "Warning" // начинается текстом Microsoft
    },
    // Правила для Console провайдера
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    // Правила для File провайдера
    "File": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  }
}
}